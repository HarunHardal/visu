{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "NodeNext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": [
      "@types/three",
      "@react-three/fiber"
    ],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "noEmit": true,
    "moduleResolution": "NodeNext"
  },
  "include": [
    "src",
    "global.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/background/Sphere.jsx",
    "src/components/sticky/Sticky.jsx",
    "src/components/spheries/Chart.tsx",
    "src/app/iletisim",
    "src/components/modelViewer/ModelViewer.tsx",
    "src/declarations.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}